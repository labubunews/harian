{
    "git.allowForcePush": true,
    "git.allowNoVerifyCommit": true,
    "git.alwaysShowStagedChangesResourceGroup": true,
    "files.associations": {
        "*.yml": "github-actions-workflow"
    },
    "github.copilot.chat.reviewSelection.instructions": [],
    "workbench.editorAssociations": {
        "*.yml": "default"
    },
    "git.autofetch": true,
    "settingsSync.ignoredSettings": [
        "github.copilot.enable"
    ],
    "githubIssues.queries": [
        {
            "label": "My Issues",
            "query": "is:open assignee:${user} repo:${owner}/${repository}",
            "groupBy": [
                "milestone"
            ]
        },
        {
            "label": "Created Issues",
            "query": "author:${user} state:open repo:${owner}/${repository} sort:created-desc"
        },
        {
            "label": "Recent Issues",
            "query": "state:open repo:${owner}/${repository} sort:updated-desc"
        }
    ],
    "[jsonc]": {
        "editor.autoIndent": "full",
        "breadcrumbs.showArrays": true,
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace"
    },
    "gitlens.currentLine.uncommittedChangesFormat": "✏️ ${ago}",
    "files.autoSave": "off",
    "redhat.telemetry.enabled": true,
    "githubIssues.alwaysPromptForNewIssueRepo": true,
    "github.codespaces.allowUnsafeConnections": true,
    "github.codespaces.createCodespaceInNewWindow": true,
    "github.codespaces.showPerformanceExplorer": true,
    "github.codespaces.supportProxies": true,
    "gitlens.advanced.messages": {
        "suppressFileNotUnderSourceControlWarning": true
    },
    "gitlens.ai.model": "openai:o3-mini",
    "notebook.defaultFormatter": "GitHub.copilot-chat",
    "git.blame.editorDecoration.enabled": true,
    "window.newWindowProfile": "Default",
    "remoteHub.commitDirectlyWarning": "auto",
    "gitlens.views.patchDetails.files.layout": "tree",
    "workbench.startupEditor": "none",
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.wrapTabs": true,
    "workbench.editor.alwaysShowEditorActions": true,
    "workbench.editor.autoLockGroups": {
        "default": true,
        "workbench.editor.chatSession": true,
        "workbench.editorinputs.searchEditorInput": true,
        "repl": true,
        "workbench.editors.gettingStartedInput": true,
        "vscode.audioPreview": true,
        "imagePreview.previewEditor": true,
        "jsProfileVisualizer.cpuprofile.table": true,
        "mainThreadWebview-markdown.preview": true,
        "vscode.videoPreview": true,
        "workbench.input.interactive": true,
        "jsProfileVisualizer.heapprofile.table": true,
        "jsProfileVisualizer.heapsnapshot.table": true,
        "jupyter-notebook": true,
        "gitlens.rebase": true
    },
    "workbench.editor.defaultBinaryEditor": "default",
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.sharedViewState": true,
    "workbench.editor.preferHistoryBasedLanguageDetection": true,
    "workbench.editor.limit.perEditorGroup": true,
    "workbench.editor.limit.enabled": true,
    "workbench.editor.limit.excludeDirty": true,
    "workbench.editor.closeOnFileDelete": true,
    "workbench.editor.centeredLayoutFixedWidth": true,
    "workbench.colorTheme": "Copilot Theme - Higher Contrast",
    "workbench.settings.applyToAllProfiles": [
        "terminal.integrated.commandsToSkipShell",
        "inlineChat.lineEmptyHint",
        "inlineChat.finishOnType",
        "accessibility.verbosity.inlineChat"
    ],
    "accessibility.verbosity.inlineChat": false,
    "security.workspace.trust.enabled": false,
    "gitlens.graph.minimap.enabled": false,
    "php.validate.executablePath": "",
    "remoteHub.experimental.useDiffMatchPatch": true,
    "remoteHub.richNavigation.enabled": true,
    "remoteHub.experimental.syncRecentList": true,
    "remoteHub.richNavigation.indexMode": {
        "branch": "latest"
    },
    "remote.SSH.useCurlAndWgetConfigurationFiles": true,
    "github.copilot.advanced": {
        "authPermissions": "default"
    },
    "gitlens.codeLens.scopes": [
        "document",
        "containers",
        "blocks"
    ],
    "gitlens.graph.dimMergeCommits": true,
    "gitlens.graph.showRemoteNames": true
}
